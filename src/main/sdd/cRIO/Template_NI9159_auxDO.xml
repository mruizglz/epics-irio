<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://www.iter.org/CODAC/PlantSystemIandC/2014-2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="NI9159_auxDO" description="NI9159 auxDO Template"  usesSubstitutionFiles="true"  version="530"  userCanInstanciate="true" xsi:schemaLocation="http://www.iter.org/CODAC/PlantSystemIandC/2014-2 ../xsd/PlantSystemIandC.xsd">
