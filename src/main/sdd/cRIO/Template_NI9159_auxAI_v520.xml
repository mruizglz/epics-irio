	<location>rio-auxai.template</location>
	<EPICSDBDName>irioAsyn.dbd</EPICSDBDName>
	<EPICSLibraryName>irio-epics</EPICSLibraryName>
   <variableTemplate name="$(CBS1)-$(CBS2)-HWCF:$(BOARDTYPE)-$(BOARDTYPEIDX)-auxAI$(CHIDX)"
